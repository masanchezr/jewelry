<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns='http://dozer.sourceforge.net'
	xsi:schemaLocation='http://dozer.sourceforge.net 
                   http://dozer.sourceforge.net/schema/beanmapping.xsd'>
	<mapping>
		<class-a>com.je.jsboot.forms.Sale</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.SaleEntity</class-b>
		<field
			custom-converter="com.je.jsboot.services.converters.StringToDateConverter">
			<a>saledate</a>
			<b>creationdate</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.services.shoppings.Shopping</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.ShoppingEntity</class-b>
		<field
			custom-converter="com.je.jsboot.services.converters.StringToDateConverter">
			<a>creationdate</a>
			<b>creationdate</b>
		</field>
		<field>
			<a>id</a>
			<b>idshop</b>
		</field>
		<field>
			<a>amount</a>
			<b>totalamount</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.services.pawns.NewPawn</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.PawnEntity</class-b>
		<field
			custom-converter="com.je.jsboot.services.converters.StringToDateConverter">
			<a>creationdate</a>
			<b>creationdate</b>
		</field>
		<field>
			<a>id</a>
			<b>idpawn</b>
		</field>
	</mapping>
	<mapping trim-strings="true">
		<class-a>com.je.jsboot.services.pawns.NewPawn</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.ClientPawnEntity</class-b>
		<field
			custom-converter="com.je.jsboot.services.converters.StringToDateConverter">
			<a>datebirth</a>
			<b>datebirth</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.services.holidays.Holiday</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.HolidayEntity</class-b>
		<field
			custom-converter="com.je.jsboot.services.converters.StringToDateConverter">
			<a>dateholiday</a>
			<b>holiday</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.services.incidents.Incident</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.IncidentEntity</class-b>
		<field>
			<a>user</a>
			<b>username.username</b>
		</field>
		<field>
			<a>date</a>
			<b>creationdate</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.forms.SalePostPoned</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.SalePostponedEntity</class-b>
		<field>
			<a>idsale</a>
			<b>idsalepostponed</b>
		</field>
		<field>
			<a>total</a>
			<b>totalamount</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.services.pawns.Pawn</class-a>
		<class-b>com.je.jsboot.dbaccess.entities.PawnEntity</class-b>
		<field>
			<a>id</a>
			<b>idpawn</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.dbaccess.entities.DiscountEntity</class-a>
		<class-b>com.je.jsboot.services.discounts.Discount</class-b>
		<field>
			<a>discount</a>
			<b>sdiscount</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.je.jsboot.dbaccess.entities.AdjustmentEntity</class-a>
		<class-b>com.je.jsboot.services.adjustments.Adjustment</class-b>
		<field>
			<a>place.description</a>
			<b>user</b>
		</field>
	</mapping>
</mappings>